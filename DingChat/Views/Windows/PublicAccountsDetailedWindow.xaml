<Window x:Class="cn.lds.chatcore.pcw.Views.Windows.PublicAccountsDetailedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cn.lds.chatcore.pcw.Views.Windows"
        xmlns:control="clr-namespace:cn.lds.chatcore.pcw.Views.Control"
        mc:Ignorable="d"     Height="500" Width="500"  
        Style="{DynamicResource WindowStyleChrome}"  Loaded="Window_Loaded"
        WindowStartupLocation="CenterScreen" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="38"/>
            <RowDefinition />

        </Grid.RowDefinitions>
        <!--关闭按钮-->
        <Button Name="BtnClose" Style="{DynamicResource xButtonStyle}" VerticalAlignment="Top" HorizontalAlignment="Right" Click="BtnClose_Click" Grid.Row="0" />
        <control:DingFrame  x:Name="frame" Grid.Row="1"></control:DingFrame>
    </Grid>
</Window>
